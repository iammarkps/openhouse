{
  "plugins": ["react"],
  "settings": {
    "react": {
      "version": "15.0"
    }
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": { "react/prop-types": 0, "react/react-in-jsx-scope": 0 },
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
